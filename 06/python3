fish_counts = [0] * 9

for fish in [int(x) for x in input().split(',')]:
    fish_counts[fish] += 1

for _ in range(256):
    spawners, fish_counts = fish_counts[0], fish_counts[1:]
    fish_counts[6] += spawners
    fish_counts.append(spawners)

print(sum(fish_counts))
